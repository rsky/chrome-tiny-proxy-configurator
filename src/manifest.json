{
  "name": "Tiny Proxy Configurator",
  "version": "1.0",
  "description": "A tiny and simple proxy configurator.",
  "permissions": [
    "proxy",
    "storage"
  ],
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon32.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
